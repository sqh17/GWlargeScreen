							}
							if(all_obj.length == 0) {
								return all_obj;
							}
							var last_obj = all_obj[all_obj.length - 1];
							all_obj.pop();
							var new_obj = {
								xData: [],
								yData1: []
							}
							for(var i = 0; i < last_obj.xData.length; i++) {
								if(typeof last_obj.xData[i] !== 'undefined') {
									new_obj.xData.push(last_obj.xData[i]);
									new_obj.yData1.push(last_obj.yData1[i]);
								}
							}
							all_obj.push(new_obj);
							return all_obj;
						}
						// 将ajax的数据转化为自己想要的数据 ---------------- 各单位先进典型数量分布
						function gdwxjdxslfb_transform(data, num) {
							var y_data1 = [],
								y_data2 = [],
								name = [],
								length = data.length,
								all_obj = [];
							for(var i = 0; i < length; i++) {
								y_data1.push(data[i].JTRY);
								y_data2.push(data[i].GRRY)
								name.push(data[i].PAR_SHORT_NAME);
							}
							var page_length = Math.ceil(name.length / num);
							for(var i = 0; i < page_length; i++) {
								all_obj[i] = {
									xData: [],
									yData1: [],
									yData2: [],
								};
								if(page_length == 1) {
									for(var j = i; j < name.length; j++) {
										all_obj[i].xData.push(name[j]);
										all_obj[i].yData1.push(y_data1[j]);
										all_obj[i].yData2.push(y_data2[j]);
									}
								} else {
									for(var j = i * num; j < i * num + num; j++) {
										all_obj[i].xData.push(name[j]);
										all_obj[i].yData1.push(y_data1[j]);
										all_obj[i].yData2.push(y_data2[j]);
									}
								}

							}
							if(all_obj.length == 0) {
								return all_obj;
							}
							var last_obj = all_obj[all_obj.length - 1];
							all_obj.pop();
							var new_obj = {
								xData: [],
								yData1: [],
								yData2: [],
							}
							for(var i = 0; i < last_obj.xData.length; i++) {
								if(typeof last_obj.xData[i] !== 'undefined') {
									new_obj.xData.push(last_obj.xData[i]);
									new_obj.yData1.push(last_obj.yData1[i]);
									new_obj.yData2.push(last_obj.yData2[i])
								}
							}
							all_obj.push(new_obj);
							return all_obj;
						}

