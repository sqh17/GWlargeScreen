						// 将ajax的数据转化为自己想要的数据  --------三会一课
						function shyk_transform(data, num) {
							var wcl = [],
								chl = [],
								name = [],
								length = data.length,
								all_obj = [];
							for(var i = 0; i < length; i++) {
								wcl.push(data[i].WCL);
								chl.push(data[i].CHL);
								name.push(data[i].PAR_SHORT_NAME);
							}
							var page_length = Math.ceil(name.length / num);
							for(var i = 0; i < page_length; i++) {
								all_obj[i] = {
									xData: [],
									finishRate: [],
									participantRate: []
								};
								if(page_length == 1) {
									for(var j = i; j < name.length; j++) {
										all_obj[i].xData.push(name[j]);
										all_obj[i].finishRate.push(wcl[j]);
										all_obj[i].participantRate.push(chl[j]);
									}
								} else {
									for(var j = i * num; j < i * num + num; j++) {
										all_obj[i].xData.push(name[j]);
										all_obj[i].finishRate.push(wcl[j]);
										all_obj[i].participantRate.push(chl[j]);
									}
								}

							}
							if(all_obj.length == 0) {
								return all_obj;
							}
							var last_obj = all_obj[all_obj.length - 1];
							all_obj.pop();
							var new_obj = {
								xData: [],
								finishRate: [],
								participantRate: []
							}
							for(var i = 0; i < last_obj.xData.length; i++) {
								if(typeof last_obj.xData[i] !== 'undefined') {
									new_obj.xData.push(last_obj.xData[i]);
									new_obj.finishRate.push(last_obj.finishRate[i]);
									new_obj.participantRate.push(last_obj.participantRate[i])
								}
							}
							all_obj.push(new_obj);
							return all_obj;
						}
