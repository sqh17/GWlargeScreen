						function dp3_zhuzi(id, zhu_data, ybeinum) {
							//最大值
							var ydatamax = Math.max.apply(null, zhu_data.ydata);
							if(ydatamax <= ybeinum) {
								var num = 1;
								jc_ydata();
							} else {
								// var num=最大值/y轴区间数
								var num = Math.ceil(ydatamax / ybeinum);
								jc_ydata();
							}
							//x轴名字回显
							$wrapper.find(id + ' .dp3_zhu_x li').empty(); //清空 复位
							$wrapper.find(id + ' .dp3_zhu_x li').each(function(i) {
								$(this).text(zhu_data.xdata[i]);
							});
							//y轴回显
							function jc_ydata() {
								//保持10的倍数
								if(num % 2 != 0) {
									num = parseInt(num) + 1
								} else {
									num = num
								};
								//y轴刻度回显
								$wrapper.find(id + ' .dp3_zhu_y li').each(function(i) {
									$(this).text(num * parseInt(ybeinum - i));
								});
								//x轴数字及百分比回显
								$wrapper.find(id + ' .zhu_ul li').each(function(i) {
									var percent = parseFloat((zhu_data.ydata[i] * 100 / (num * ybeinum)));
									var $top = $(this).find('.bar-main-top');
									var $down = $(this).find('.bar-main-down');
									//清空 复位
									$(this).find('.bar-num').text(' ');
									$(this).find('.bar-main').height(0);
									$top.hide();
									$down.hide();
									//清空结束
									$(this).find('.bar-num').text(zhu_data.ydata[i]);
									//console.log(num * ybeinum) y轴最大坐标值
									$(this).find('.bar-main').height(percent + '%');
									if(percent == 0) {
										$top.hide();
										$down.hide();
									} else if(!isNaN(percent)) {
										$top.show();
										$down.show();
									}
								})
							};
						};

